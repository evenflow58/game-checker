{
  "name": "settings",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "Evan Johnson",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all --parallel build:* && cd ../",
    "build:get": "cd get && npm run build",
    "build:put": "cd put && npm run build",
    "install": "npm-run-all --parallel install:*",
    "install:get": "cd get && npm i",
    "install:put": "cd put && npm i",
    "package": "npm-run-all package:*",
    "package:settings": "sam package --template-file template.yaml --s3-bucket $S3_BUCKET --s3-prefix url --output-template-file ./template.package.yaml",
    "package:get": "cd get && npm run package",
    "package:put": "cd put && npm run package",
    "test": "npm-run-all test:*",
    "test:get": "cd get && npm run test"
  }
}
